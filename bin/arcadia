require 'rubygems' # for a few dependencies
$pwd = Dir.pwd
Dir.chdir("#{File.dirname(__FILE__)}/..")
if FileTest.exist?('conf/arcadia.init.rb')
  require 'conf/arcadia.init'
end
require "lib/a-core"
Arcadia.new.run
